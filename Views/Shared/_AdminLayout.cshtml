
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"]</title>
    <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet">
    <link rel="stylesheet" href="~/sbadmin/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/sbadmin/css/site.css" />
    <link rel="stylesheet" href="~/sbadmin/css/sb-admin-2.min.css" />
    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/v/dt/dt-1.11.2/r-2.2.9/datatables.min.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css" integrity="sha512-1ycn6IcaQQ40/MKBW2W4Rhis/DbILU74C1vSrLJxCq57o941Ym01SwNsOMqvEBFlcgUa6xLiPY/NS5R+E6ztJQ==" crossorigin="anonymous" referrerpolicy="no-referrer" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/css/toastr.min.css" />
    <!-- Ultronicon-->
    <link rel="icon" type="image/x-icon" href="~/edufavicon.ico" />
</head>

<body id="page-top">
    <div id="wrapper">
        <!-- Sidebar -->

        <partial name="_SideBar" />
        <!-- Content Wrapper -->

        <div id="content-wrapper" class="d-flex flex-column">
            <!-- Main Content -->
            <div id="content">
                <partial name="_TopBar" />
                <div class="container-fluid">
                    @RenderBody()
                </div>
            </div>

            <!-- Footer -->
            <footer class="sticky-footer bg-white">
                <div class="container my-auto">
                    &copy; 2021 - ultra - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
                </div>
            </footer>
            <!-- End of Footer -->
        </div>
    </div>

    <script src="~/sbadmin/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/sbadmin/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/sbadmin/js/site.js" asp-append-version="true"></script>
    <script type="text/javascript" src="https://cdn.datatables.net/v/dt/dt-1.11.2/r-2.2.9/datatables.min.js"></script>
    <script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/js/toastr.min.js"></script>

    <script src="~/sbadmin/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/sbadmin/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/sbadmin/js/site.js" asp-append-version="true"></script>

    <script src="~/sbadmin/js/sb-admin-2.min.js" asp-append-version="true"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-easing/1.4.1/jquery.easing.min.js"></script>

    <script type="text/javascript" src="https://cdn.datatables.net/v/dt/dt-1.11.2/r-2.2.9/datatables.min.js"></script>
    <script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/js/toastr.min.js"></script>

@await RenderSectionAsync("Scripts", required: false)
</body>
</html>